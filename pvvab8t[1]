var _uxQuotes = _interopRequireDefault(__webpack_require__(/*! @balsamiq/ux-quotes */ "./node_modules/@balsamiq/ux-quotes/ux-quotes.js"));

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

function _defineProperty(obj, key, value) { if (key in obj) { Object.defineProperty(obj, key, { value: value, enumerable: true, configurable: true, writable: true }); } else { obj[key] = value; } return obj; }

function pickRandomUxQuote() {
  return _uxQuotes.default[Math.floor(Math.random() * _uxQuotes.default.length)];
}

const BalsamiqMockupsLogo = `<svg viewBox="0 0 256 256" xmlns="http://www.w3.org/2000/svg"><g fill="none" fill-rule="evenodd"><g fill="#3E3E3E"><path d="M197.304 28.107c39.85 26.943 65.926 79.84 46.565 142.726-27.199 88.344-166.328 113.295-216.316 49.45C-14.12 167.049 9.936 38.723 63.607 17.985 77.234 12.718 118.36 9.709 134.48 9.1a101.92 101.92 0 0 1-3.77-2.177c-3.366-2.05 11.675-9.095 16.308-6.272 15.915 9.69 36.143 14.971 50.286 27.455zM36.438 201.963c19.64 32.486 79.438 43.574 121.904 31.502 68.99-19.614 104.95-132.684 40.63-184.5a200.718 200.718 0 0 0-7.62-4.866c-1.286-.83-10.797-6.132-15.061-8.177-4.009 2.03-9.933 4.018-12.6 2.67-8.048-4.071-15.895-8.818-24.345-12.041-5.132-1.957-9.97-2.892-14.845-3.436-18.53-.25-37.132 4.018-56.46 12.35-46.4 20-58.259 122.41-31.603 166.498z"/><path d="M88.959 116.225c-17.559 4.153-19.161-19.352-9.006-22.5 17.339-5.216 20.889 19.498 9.006 22.5zM176.555 101.45c4.052 10.806-7.573 19.593-15.35 15.709-6.895-3.552-8.889-15.71-2.43-20.153 6.458-4.444 14.146-3.173 17.78 4.444zM56.29 138.498c0-1.504.28-2.813 3.024-3.166 2.39-.309 3.26 4.16 3.798 5.764 10.057 30.491 85.446 54.362 121.984 6.465 1.158-1.556 2.753-4.438 4.662-3.702 1.909.736 1.46 1.688 1.175 4.338-9.99 65.303-134.925 52.847-134.643-9.7z"/></g></g></svg>`; // eslint-disable-next-line

const css = s => s[0];

const style = css`
    @-webkit-keyframes OperationProgressFrames {
        0% {
            -webkit-transform: translateX(0);
            transform: translateX(0);
        }
        100% {
            -webkit-transform: translateX(90%);
            transform: translateX(90%);
        }
    }

