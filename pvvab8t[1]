const containerId = "balsamiq-loading-screen";
const elementsContainerId = `${containerId}-elementsContainer`;
const titleId = `${containerId}-title`;
const quoteId = `${containerId}-quote`;
const progressId = `${containerId}-progress`;
const INDETERMINATE_PROGRESS = -1;
const DEFAULT_TITLE = "Loading…";
const DefaultBackgroundColor = "#e5e5e5";
const DefaultTopBarBackgroundColor = "#bbbbbb";

class LoadingScreen {
  constructor() {
    _defineProperty(this, "parentEl", void 0);

    _defineProperty(this, "containerEl", void 0);

    _defineProperty(this, "styleEl", void 0);

    _defineProperty(this, "isRemovingContainerEl", false);

    _defineProperty(this, "currentQuote", void 0);

    _defineProperty(this, "currentProgress", INDETERMINATE_PROGRESS);

    _defineProperty(this, "title", DEFAULT_TITLE);

    _defineProperty(this, "startsWithQuote", false);

    _defineProperty(this, "onHideCallback", void 0);

    this.parentEl = document.querySelector("body");
    this.styleEl = this.createStyle();
    this.containerEl = this.createMain(); // this.show();
  }

  show() {
    let options = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : {
      fadein: false,
      topbar: false,
      small: false,
      showQuote: false
    };
    if (this.parentEl.contains(this.containerEl)) return false;
    const backgroundColor = options.backgroundColor || DefaultBackgroundColor;
    this.parentEl.appendChild(this.styleEl);
    this.parentEl.appendChild(this.containerEl);

    if (options.small) {
      this.containerEl.classList.add("small");
    }

    this.containerEl.style.backgroundColor = backgroundColor;
    const topBarEl = document.querySelector("#loadingScreenTopBar");
    topBarEl.style.backgroundColor = options.topbar ? DefaultTopBarBackgroundColor : backgroundColor;

    if (options.fadein) {
      window.requestAnimationFrame(() => {
        // double requestAnimationFrame for Chrome, because it consumes the first one in the current rendering
        window.requestAnimationFrame(() => {
          this.containerEl.classList.add("opacity1");
        });
      });
    } else {
      // adding the opacity1 class immediately does not trigger the animation
      this.containerEl.classList.add("opacity1");
    }

    if (options.showQuote) {
      this.showQuote(pickRandomUxQuote());
      this.startsWithQuote = true;
    }

    return true;
  }

  hide() {
    if (this.containerEl.parentElement && !this.isRemovingContainerEl) {
      this.isRemovingContainerEl = true;
      this.containerEl.classList.remove("opacity1");

      const removeElements = () => {
        this.setTitle(DEFAULT_TITLE);
        this.containerEl.removeEventListener("transitionend", removeElements);
        const existingQuoteNode = this.containerEl.querySelector(`#${quoteId}`);

        if (existingQuoteNode) {
          existingQuoteNode.parentNode && existingQuoteNode.parentNode.removeChild(existingQuoteNode);
        }

        this.parentEl.removeChild(this.containerEl);
        this.parentEl.removeChild(this.styleEl);
        this.currentQuote = undefined;
        this.isRemovingContainerEl = false;
        this.containerEl = this.createMain();
      };

      this.containerEl.addEventListener("transitionend", removeElements);
    }

    this.currentProgress = INDETERMINATE_PROGRESS;
    this.title = DEFAULT_TITLE;
    this.startsWithQuote = false;

    if (this.onHideCallback) {
      this.onHideCallback();
      this.onHideCallback = undefined;
    }
  }

  showProgress(value) {
    if (this.currentProgress === value) {
      return;
    }

    this.currentProgress = value;
    const progressEl = this.containerEl.querySelector(`#${progressId}`);
    progressEl && progressEl.parentNode && progressEl.parentNode.replaceChild(this.createProgress({
      progress: value
    }), progressEl);
  }

  showQuote(quote) {
    if (this.startsWithQuote || this.currentQuote && this.currentQuote.quote === quote.quote) {
      return;
    }

    this.currentQuote = quote;
    const existingQuoteNode = this.containerEl.querySelector(`#${quoteId}`);

    if (existingQuoteNode) {
      existingQuoteNode.parentNode && existingQuoteNode.parentNode.removeChild(existingQuoteNode);
    }

    this.containerEl.querySelector(`#${elementsContainerId}`).appendChild(this.createQuote(quote));
  }

  setTitle(title) {
    if (this.title === title) {
      return;
    }

    this.title = title;
    this.containerEl.querySelector(`#${titleId}`).textContent = title;
  }

  registerOnHide(callback) {
    this.onHideCallback = callback;
    return () => {
      this.onHideCallback = undefined;
    };
  }

  createStyle() {
    const styleEl = document.createElement("style");
    styleEl.innerHTML = style;
    return styleEl;
  }

  createMain() {
    const containerEl = document.createElement("div");
    containerEl.id = containerId;
    containerEl.className = "mainContainer";
    containerEl.appendChild(this.createTopBar());
    const elementsContainer = document.createElement("div");
    elementsContainer.id = elementsContainerId;
    elementsContainer.className = "elementsContainer";
    elementsContainer.appendChild(this.createLogo());
    elementsContainer.appendChild(this.createProgress({
      progress: INDETERMINATE_PROGRESS
    }));
    containerEl.appendChild(elementsContainer);
    return containerEl;
  }

  createTopBar() {
    const container = document.createElement("div");
    container.innerHTML = `<div class="topBar" id="loadingScreenTopBar"></div>`;
    return container.firstElementChild;
  }

  createLogo() {
    const container = document.createElement("div");
    container.innerHTML = `
        <div class="logo">
            ${BalsamiqMockupsLogo}
        </div>
        `;
    return container.firstElementChild;
  }

  createProgress(_ref) {
    let {
      progress
    } = _ref;
    const isIndeterminate = progress === INDETERMINATE_PROGRESS;
    const container = document.createElement("div");
    container.innerHTML = `
        <div id="${progressId}" class="progress">
            <p id="${titleId}" class="progressText">${this.title}</p>
            <div class="progressContainer">
                <div class="progressWrapper ${isIndeterminate ? "indefiniteProgressWrapper" : ""}">
                    <div style="${isIndeterminate ? "" : `width: ${progress * 100}`}%" class="${isIndeterminate ? "indefiniteProgressBar" : "steppedProgressBar"}"></div>
                </div>
            </div>
        </div>
        `;
    return container.firstElementChild;
  }

  createQuote(_ref2) {
    let {
      quote,
      author,
      url
    } = _ref2;
    const container = document.createElement("div");
    container.innerHTML = `
        <div class="quote" id="${quoteId}">
            <div class="quoteContainer">
                <p class="quoteText">${quote}</p> 
                <p class="quoteText quoteAuthor">&#8212;&nbsp;<span>${author}</span></p>
            </div>
            <div class="quoteReadmeButtonContainer">
                <a href="${url}" target="_blank" rel="noopener noreferrer" role="button" class="quoteReadmeButton">Read More…</a>
            </div>
        <div>
        `;
    return container.firstElementChild;
  }

}

let loadingScreen;
window.BalsamiqLoader = {
  getOrCreateLoader() {
    if (!loadingScreen) {
      loadingScreen = new LoadingScreen();
    }

    return loadingScreen;
  }

};
})();

